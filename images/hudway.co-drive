    if (amplitude !== null) {
    amplitude.getInstance().init("f341235d5e89a86f0cb860499228f301", null, {
        saveEvents: true,
        includeUtm: true,
        includeReferrer: true,
    });
    
    let pageView = document.location.pathname.slice(1);

    if (pageView[pageView.length - 1] === "/") {
        pageView = pageView.substring(0, pageView.length - 1)
    }

    if (pageView === "") {
        pageView = "main";
    }

    amplitude.getInstance().logEvent('page_view', {page: pageView.toLowerCase()});

    function $_GET(key) {
        let p = window.location.search.toLowerCase();
        p = p.match(new RegExp(key + '=([^&=]+)'));
        return p ? p[1] : false;
    }

    function getCookie(name) {
        let matches = document.cookie.match(new RegExp(
            "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
        ));
        return matches ? decodeURIComponent(matches[1]) : undefined;
    }

    let identify = new amplitude.Identify();
    let date = new Date();

    identify.setOnce("First Visit", date.getFullYear().toString() + "/" + (date.getMonth() + 1).toString() + "/" + date.getDate().toString());

    if (getCookie('hudwayLastVisit') !== undefined) {
        let date = new Date(parseFloat(getCookie('hudwayLastVisit')));
        identify.set("Last Visit", date.getFullYear().toString() + "/" + (date.getMonth() + 1).toString() + "/" + date.getDate().toString());
    } else {
        document.cookie = encodeURIComponent("hudwayLastVisit") + "=" + encodeURIComponent(Date.now().toString()) + ";max-age=31556926;domain=hudway.co";
    }

    /* 
    try {
        identify.set("Number of Visits", amplitude.getInstance()._eventId);
    }catch(e) {}
    */

    if (document.referrer !== "") {
        let link = new URL(document.referrer);
        identify.set("Referer domain", link.hostname);
        identify.set("Referer URL", link.hostname + link.pathname);
    }

    if ($_GET('utm_campaign') !== false)
        identify.set("UTM_Campaign", $_GET('utm_campaign'));

    if($_GET('utm_source') !== false)
        identify.set("UTM_Source", $_GET('utm_source'));

    if($_GET('utm_medium') !== false)
