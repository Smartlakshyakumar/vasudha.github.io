    if($_GET('utm_content') !== false)
        identify.set("UTM_Content", $_GET('utm_content'));

    if($_GET('utm_term') !== false)
        identify.set("UTM_Term", $_GET('utm_term'));

    amplitude.getInstance().identify(identify);
    console.log(["Amplitude log", {page: pageView}, identify]);

}
